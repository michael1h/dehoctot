{
  "appId": "toeic.full.pro.lamdt4",
  "appName": "Toeic Guru",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "a",
      "iconColor": "#488AFF",
      "sound": "beep.wav"
    },
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "backgroundColor": "#ffffffff",
      "androidSplashResourceName": "a",
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "large",
      "spinnerColor": "#f70808",
      "showSpinner": true,
      "splashFullScreen": false,
      "splashImmersive": true
    }
  }
}
